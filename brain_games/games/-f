import random


def fn():
    start = random.randint(1, 12)
    step = random.randint(2, 5)
    stop = start + step * 10
    progression = list(range(start, stop, step))
    hidden_number = random.choice(progression)
    progr_str = " ".join(map(str, progression))
    changed_progr = progr_str.replace(str(hidden_number), '..')
    return changed_progr
print(fn())
